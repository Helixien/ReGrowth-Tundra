<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_TundraGrass</defName>
		<label>grass</label>
		<description>Wild tundra grass. Grows in colder regions where there is a little light and minimally fertile ground.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Flammability>1.2</Flammability>
			<Nutrition>0.20</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/TundraGrass</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.3</fertilitySensitivity>
			<growDays>2.5</growDays>
			<leaflessGraphicPath>Things/Plant/Grass_Leafless</leaflessGraphicPath>
			<harvestWork>40</harvestWork>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.4~0.6</visualSizeRange>
			<topWindExposure>0.4</topWindExposure>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_TundraTallGrass</defName>
		<label>tall grass</label>
		<description>Wild tall tundra grass. Slows down anyone moving over it.</description>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<Flammability>1.3</Flammability>
			<Nutrition>0.25</Nutrition>
		</statBases>
		<pathCost>14</pathCost>
		<graphicData>
			<texPath>Things/Plant/TundraTallGrass</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.7</fertilitySensitivity>
			<growDays>3.5</growDays>
			<harvestWork>70</harvestWork>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.7~0.85</visualSizeRange>
			<topWindExposure>0.4</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>10</wildClusterWeight>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>
