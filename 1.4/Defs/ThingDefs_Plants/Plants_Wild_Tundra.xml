		</statBases>
		<ingestible/>
		<plant>
			<growDays>20</growDays>
			<visualSizeRange>1.1~2.5</visualSizeRange>
			<harvestWork>800</harvestWork>
			<harvestYield>27</harvestYield>
		</plant>
	</ThingDef>
